package projekat1;


public class Main {

    public static void main(String[] args) {

       for (int i = 0; i<5;i++){
           for (int a=0;a<i;a++){
               System.out.print(" ");
           }
           for (int b=5-i;b>0;b--){
               System.out.print("*");
           }
           System.out.println();

       }

}}



